{
    "name": "ep-api",
    "version": "0.0.0",
    "description": "description",
    "author": "",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "seed": "tsc && node dist/seed.js",
        "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "cross-env NODE_ENV=development nodemon",
        "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
        "prestart:prod": "rimraf dist && tsc",
        "start:prod": "cross-env NODE_ENV=production node dist/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^5.4.1",
        "@nestjs/core": "^5.4.1",
        "@nestjs/elasticsearch": "^0.2.0",
        "@nestjs/swagger": "^2.5.1",
        "@nestjs/websockets": "^5.4.1",
        "@types/mongodb": "^3.1.15",
        "@types/mysql": "^2.15.5",
        "@types/ws": "^6.0.1",
        "chalk": "^2.4.1",
        "cheerio": "^1.0.0-rc.2",
        "cors": "^2.8.5",
        "dotenv": "^6.1.0",
        "event-stream": "^4.0.1",
        "htmldiff-js": "^1.0.5",
        "ipfs-http-client": "^27.0.0",
        "joi": "^14.3.0",
        "lodash": "^4.17.11",
        "mongodb": "^3.1.10",
        "mongoose": "^5.3.14",
        "morgan": "^1.9.1",
        "mysql": "^2.16.0",
        "node-fetch": "^2.3.0",
        "ps-node": "^0.1.6",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "rxjs": "^6.3.3",
        "typescript": "^3.2.1",
        "ws": "^6.1.2"
    },
    "devDependencies": {
        "@nestjs/testing": "^5.1.0",
        "@types/dotenv": "^6.1.0",
        "@types/events": "^1.2.0",
        "@types/express": "^4.16.0",
        "@types/jest": "^23.3.10",
        "@types/joi": "^14.0.0",
        "@types/lodash": "^4.14.118",
        "@types/morgan": "^1.7.35",
        "@types/node": "^10.12.11",
        "@types/supertest": "^2.0.6",
        "cross-env": "^5.2.0",
        "jest": "^23.6.0",
        "nodemon": "^1.18.7",
        "prettier": "^1.15.3",
        "supertest": "^3.3.0",
        "ts-jest": "^23.10.5",
        "ts-loader": "^4.4.2",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.5.0",
        "tslint": "5.11.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
