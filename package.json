  "scripts": {
    "build": "cross-env-shell NODE_ENV=production \"tsc --p ./tsconfig.json\"",
    "build:sync": "cross-env-shell NODE_ENV=production \"tsc --p ./tssync.json\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "tsc && node dist/seed.js",
    "sync": "node ./dist/scripts/synconly.js",
    "start": "cross-env-shell NODE_ENV=production \"node ./dist/src/main.js\"",
    "dev": "env-cmd .env nodemon",
    "start:debug": "npm run build:sync && node ./dist/scripts/synconly.js & > /dev/null && cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
    "maintenance:run-patch": "tsc --p ./tsconfig.json && node ./dist/scripts/Non-Lambda/Page-Categorizer-Universal.js -s 1 -e 10000",
    "maintenance:run-wiki-deindexer": "tsc --p ./tsconfig.json && node ./dist/scripts/Non-Lambda/Wikipedia-Import-Deindexer.js -s 0 -e 1000000",
    "maintenance:run-wiki-deleted-page-finder": "tsc --p ./tsconfig.json && node ./dist/scripts/Wiki-Deleted-Page-Finder/Wiki-Deleted-Page-Finder.js -s 943911 -e 1000000",
    "maintenance:run-wiki-import": "tsc --p ./tsconfig.json && node ./dist/scripts/Wiki-Importer/Wiki-Import.js -s 1529596 -e 1529597",
    "maintenance:run-sync-since": "tsc --p ./tsconfig.json && node ./dist/scripts/Wiki-Sync-Since/Wiki-Sync-Since.js -s 1569888000 -e 1570838400",
    "maintenance:create-webp": "tsc --p ./tsconfig.json && node ./dist/scripts/Non-Lambda/Make-WebP-Images.js --input '../../../scripts/Non-Lambda/input/slug-list.txt'",
    "maintenance:batch": "tsc --p ./tsconfig.json && python ./scripts/batch/batch-commands.py &",
    "maintenance:batch-2": "tsc --p ./tsconfig.json && python ./scripts/batch/batch-commands-2.py &",
    "maintenance:batch-truncate-logs": "truncate -s 0 ./scripts/batch/log/batch_* &",
    "maintenance:batch-burn-lines": "find ./scripts/batch/inputs/ -type f -name 'batch*' | xargs sed -i '1,925d'",
    "maintenance:batch-show-a-log": "tail -n 250 scripts/batch/log/batch_15.log",
    "maintenance:batch-show-wiki-import-log": "tail -n 250 ./scripts/Wiki-Importer/resultlinks.txt",
    "server:status": "ssh beekeeper@apitest.everipedia.org 'pm2 logs --nostream && pm2 status && systemctl status mongodb && systemctl --user status ep2-synconly'",
    "server:reload": "ssh beekeeper@apitest.everipedia.org 'pm2 reload backend'",
    "server:restart-ipfs": "ssh beekeeper@apitest.everipedia.org 'systemctl --user restart ipfs'",
    "server:restart-mongo": "ssh beekeeper@apitest.everipedia.org 'sudo systemctl restart mongodb && systemctl --user restart ipfs && pm2 reload backend && systemctl --user restart ep2-synconly'",
    "deploy:pre-install": "ssh beekeeper@apitest.everipedia.org 'sudo apt-get install -y ffmpeg build-essential libcairo2-dev libpango1.0-dev libjpeg-dev libgif-dev librsvg2-dev'",
    "deploy:prod": "ssh beekeeper@apitest.everipedia.org 'cd /home/beekeeper/ep2-backend/ && git pull origin master && npm install && npm run-script build && pm2 reload backend'",
    "deploy:backup": "ssh beekeeper@frontend.everipedia.org 'cd /home/beekeeper/ep2-backend/ && git pull origin master && npm install && npm run-script build && pm2 reload backend'"
  },
